{
  "name": "htmllinter",
  "displayName": "htmllinter",
  "description": "vscode extension for htmllinter",
  "version": "0.0.1",
  "publisher": "anikethsaha",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Linters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anikethsaha/vscode-htmllinter.git"
  },
  "bugs": {
    "url": "https://github.com/anikethsaha/vscode-htmllinter/issues"
  },
  "activationEvents": [
    "onLanguage:html",
    "htmllinter.lint"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "htmllinter.lint",
        "title": "Run htmllinter for the opened file"
      }
    ]
  },
  "keywords": [
    "lint",
    "html",
    "markup"
  ],
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "dependencies": {
    "@htmllinter/core": "^0.4.0",
    "@htmllinter/basic-config": "^0.4.0"
  },
  "devDependencies": {
    "@types/vscode": "^1.41.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.2",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
